pipeline {
  agent any
  tools {
    maven "maven_jenkins"
  }
  stages {
    stage ('Maven Clean'){
      steps{
        sh 'mvn clean'
      }
    }
    stage ('Maven package') {
      steps{
        sh 'mvn package'
      }
    }
 
 
                                 
                                 
                                 
                                 

                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
